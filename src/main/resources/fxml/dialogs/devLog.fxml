<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<DialogPane headerText="Developer Console" stylesheets="@../../styles/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox>
         <children>
            <TableView VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn editable="false" prefWidth="75.0" text="date" />
                <TableColumn editable="false" prefWidth="75.0" text="type" />
                <TableColumn editable="false" prefWidth="75.0" sortable="false" text="message" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
   </buttonTypes>
</DialogPane>
